<?xml version="1.0" encoding="UTF-8" ?>
<config>
	<app>
		<environment>development</environment>
		<guice-modules>
			jhyun.hotwind.mybatis.MyBatisXmlModule,
			jhyun.hotwind.jersey.JerseyServletModule
		</guice-modules>
		<jersey-modules>
			jhyun.hotwind.hello.controllers.HelloController
		</jersey-modules>
		<jersey-properties-file>jersey.properties</jersey-properties-file>
		<jersey-serve-url-pattern>/*</jersey-serve-url-pattern>
	</app>
	<test>
		<api-url-prefix>http://localhost:8080</api-url-prefix>
	</test>
</config>